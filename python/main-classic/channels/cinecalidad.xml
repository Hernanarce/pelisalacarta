<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<channel>
   <id>cinecalidad</id>
   <name>CineCalidad</name>

   <active>true</active>
   <adult>false</adult>
   <language>es</language>

   <fanart></fanart>
   <thumbnail>https://s31.postimg.org/puxmvsi7v/cinecalidad.png</thumbnail>
   <bannermenu>https://s32.postimg.org/kihkdpx1x/banner_cinecalidad.png</bannermenu>

   <update_url>https://raw.githubusercontent.com/tvalacarta/pelisalacarta/master/python/main-classic/channels/</update_url>
   <version>4</version>

   <changes>
		<change>
			<date>15/03/2017</date>
			<autor>SeiTaN</autor>
			<description>limpieza código</description>
		</change>
		<change>
			<date>18/06/2016</date>
			<autor>Hernan_Ar_c</autor>
			<description>First release.</description>
		</change>
	</changes>

   <categories>
      <category>latino</category>
      <category>movie</category>
   </categories>

   <settings>
      <id>include_in_global_search</id>
      <type>bool</type>
      <label>Incluir en busqueda global</label>
      <default>false</default>
      <enabled>false</enabled>
      <visible>false</visible>
   </settings>
   
   <settings>
      <id>filter_languages</id>
      <type>list</type>
      <label>Mostrar enlaces en idioma...</label>
      <default>0</default>
      <enabled>true</enabled>
      <visible>true</visible>
      <lvalues>No filtrar</lvalues>
      <lvalues>Latino</lvalues>
      <lvalues>Español</lvalues>
      <lvalues>Portuges</lvalues>
   </settings>

   
   
   <settings>
      <id>autoplay_settings</id>
      <type>bool</type>
      <label>   Configuracion Personalizada (Define tus Preferencias):</label>
      <color>0xff66ffcc</color>
      <default>false</default>
      <enabled>eq(-1,true)</enabled>
      <visible>true</visible>
   </settings>

   <settings>
      <id>priority</id>
      <type>list</type>
      <label>      - Prioridad (Indica el orden para Auto-Reproducir)</label>
      <color>0xffffff99</color>
      <default>0</default>
      <enabled>eq(-2,true)+eq(-1,true)</enabled>
      <visible>true</visible>
      <lvalues>Servidor y Calidad</lvalues>
      <lvalues>Servidor</lvalues>
      <lvalues>Calidad</lvalues>
   </settings>

   <settings>
      <id>label4</id>
      <type>label</type>
      <label></label>
      <enabled>false</enabled>
      <visible>true</visible>
   </settings>

  
   <settings>
		<id>include_in_newest_peliculas</id>
		<type>bool</type>
		<label>Incluir en Novedades - Peliculas</label>
		<default>true</default>
		<enabled>true</enabled>
		<visible>true</visible>
	</settings>
	<settings>
		<id>include_in_newest_infantiles</id>
		<type>bool</type>
		<label>Incluir en Novedades - Infantiles</label>
		<default>true</default>
		<enabled>true</enabled>
		<visible>true</visible>
	</settings>
</channel>
